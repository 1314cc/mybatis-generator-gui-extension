<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="423.0" prefWidth="347.0" stylesheets="@/style/style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spawpaw.mybatis.generator.gui.controller.DatabaseConfigEditorController">
    <VBox fx:id="main_container" prefHeight="1000.0">
      <padding>
         <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
      </padding></VBox>
    <HBox alignment="CENTER_LEFT" minHeight="48.0">
        <Button minWidth="80.0" mnemonicParsing="false" onMouseClicked="#onTestConnectionClicked" text="%ui.testConnection" />
        <HBox alignment="CENTER_LEFT" prefWidth="1000.0">
            <Label fx:id="lb_msg" />
        </HBox>
        <Button minWidth="60.0" mnemonicParsing="false" onMouseClicked="#onCancelClicked" text="%ui.cancel">
         <HBox.margin>
            <Insets right="8.0" />
         </HBox.margin></Button>
        <Button minWidth="60.0" mnemonicParsing="false" onMouseClicked="#onSaveClicked" text="%ui.save" />
      <padding>
         <Insets left="8.0" right="8.0" />
      </padding>
    </HBox>
</VBox>
